<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm" name="postTitle">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="3"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>
